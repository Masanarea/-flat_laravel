{
    "regions": [
        "hnd1"
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/index.php"
        },
        {
            "src": "/about",
            "dest": "/about"
        }
    ],
    "builds": [
        {
            "src": "public/**",
            "use": "vercel-php@0.5.2"
        }
    ],
    "env": {
        "APP_NAME": "Laravel Vercel",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://flat-laravel.vercel.app/",
        "ASSET_URL": "https://flat-laravel.vercel.app/",
        "APP_KEY": "base64:khYfW1kZKfdEsMNYdKP0rnUh6TWdj4C+Ct0a6VDe6Vc=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "LIVEWIRE_MANIFEST_PATH": "/tmp/livewire-components.php",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp"
    }
}
